{
	"name": "assistant",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "bun db:generate && bun drizzle-kit migrate",
		"db:squash": "rm -rf supabase/migrations/* && bun db:generate && bun db:migrate",
		"db:studio": "drizzle-kit studio --config drizzle.config.ts",
		"db:drop": "drizzle-kit drop"
	},
	"devDependencies": {
		"@supabase/auth-ui-shared": "^0.1.8",
		"@supabase/auth-ui-svelte": "^0.2.9",
		"@supabase/ssr": "^0.1.0",
		"@supabase/supabase-js": "^2.45.1",
		"@sveltejs/adapter-auto": "^3.2.4",
		"@sveltejs/kit": "^2.5.22",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tailwindcss/typography": "^0.5.14",
		"@types/eslint": "^8.56.11",
		"@types/node": "^20.14.15",
		"@types/pg": "^8.11.6",
		"autoprefixer": "^10.4.20",
		"debug": "^4.3.6",
		"drizzle-kit": "^0.23.2",
		"eslint": "^9.9.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"globals": "^15.9.0",
		"postcss": "^8.4.41",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.5",
		"tailwindcss": "^3.4.10",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.1.0",
		"vite": "^5.4.1"
	},
	"type": "module",
	"dependencies": {
		"@ai-sdk/anthropic": "^0.0.34",
		"@ai-sdk/google": "^0.0.30",
		"@ai-sdk/openai": "^0.0.40",
		"@ai-sdk/provider": "^0.0.18",
		"@aws-sdk/client-s3": "^3.632.0",
		"@aws-sdk/lib-storage": "^3.632.0",
		"@types/hast": "^3.0.4",
		"ai": "^3.3.7",
		"clsx": "^2.1.1",
		"daisyui": "^4.12.10",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.32.2",
		"formsnap": "^1.0.1",
		"hast": "^1.0.0",
		"katex": "^0.16.11",
		"lowlight": "^3.1.0",
		"lucide-svelte": "^0.418.0",
		"mode-watcher": "^0.4.1",
		"pg": "^8.12.0",
		"postgres": "^3.4.4",
		"rehype-highlight": "^7.0.0",
		"rehype-katex": "^7.0.0",
